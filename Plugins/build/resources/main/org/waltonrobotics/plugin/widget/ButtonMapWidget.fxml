<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.waltonrobotics.plugin.widget.ButtonMapWidget">
  <children>
    <ScrollPane fx:id="scrollPane" prefHeight="400.0" prefWidth="600.0">
      <padding>
        <Insets topRightBottomLeft="10" />
      </padding>

    </ScrollPane>
    <Button fx:id="applyButton" mnemonicParsing="false" onAction="#updateData" text="Apply" />
  </children>
</VBox>
